<div class="col-md-12">
    <table class="table table-bordered table-hover table-condensed table-responsive container-fluid" data-height="400">
        <tr>
            <th class="text-center">Compound</th>
            <th class="text-center">Synonyms</th>
            <th class="text-center">Max Phase</th>
            <th class="text-center">Mechanism of Action</th>
            <th class="text-center">Drug Classification</th>
            <th class="text-center">Study Area</th>
            <th class="text-center">Model System</th>
            <th class="text-center">Datasets</th>
        </tr>

        <tr ng-repeat="docs in datasets.documents">
            <td class="table-width">
                <div class="text-center">
                    <h3><a class="data-button" ng-href="#/view/{{docs.lincsidentifier.toString()}}">{{docs.SM_Name.toString()}}</a></h3>
                </div>
                <img style="min-height: 12em; max-height: 13em"
                     ng-src="http://life.ccs.miami.edu/life/web/images/sm-images/400/{{docs.lincsidentifier.toString()}}.png">
            </td>
            <td class="table-width text-center">
                <div ng-repeat="synomys in docs.SM_Alternative_Name track by $index">
                    {{synomys}}
                </div>

            </td>

            <td class="table-width text-center">
                {{docs.FDA_PHASE.toString()}}
            </td>
            <td class="table-width">
               {{docs.MECH_OF_ACTION}}
            </td>


            <td class="table-width">
                <div ng-repeat=" drug in docs.Drug_Class">
                    {{drug}}
                </div>

            </td>
            <!--<td class="table-width">-->
                <!--<div ng-repeat="datasets in docs.datasets">-->
                    <!--<a ng-href="/datasets/#/view/{{datasets}}">{{datasets}}</a>-->
                <!--</div>-->
            <!--</td>-->
            <td class="chart-box">
                <!--<canvas id="doughnut" width="50" height="50" class="chart chart-doughnut"-->
                         <!--chart-data="docs.subject_area_count" chart-labels="docs.subject_area" chart-colors="colors">-->
                <!--</canvas>-->
                <canvas id="base" style="max-height: 8em;max-width: 20em" class="chart-horizontal-bar"
                        chart-data="docs.subject_area_count"   chart-labels="docs.subject_area" chart-options="options2" chart-colours ="colours" chart-dataset-override="override">
                </canvas>
            </td>
            <td class="table-width">
                <h4 ng-if="docs.cells.length" style="color: darkgreen" class="text-center">{{docs.cells.length}} cell-lines used</h4>
                <div style="overflow-y:scroll; height:200px;">
                     <div class="text-center" ng-repeat="cells in docs.cells" >
                    <a ng-href="/cells/#?query=Name:{{cells}}" target="_blank">{{cells}}</a>
                     </div>
                </div>
                <!--<canvas id="doughnut" width="50" height="50" class="chart chart-doughnut"-->
                        <!--chart-data="docs.cells_count" chart-labels="docs.cells">-->
                <!--</canvas>-->
            </td>
            <td class="table-width">
                <h4 ng-if="docs.datasets.length" style="color: darkgreen" class="text-center">Used in {{docs.datasets.length}} Datasets </h4>
                <div style="overflow-y:scroll; height:200px;">
                    <div class="text-center" ng-repeat="datasets in docs.datasets" >
                        <a ng-href="/datasets/#/view/{{datasets}}" target="_blank">{{datasets}}</a>
                    </div>
                </div>
                <!--<canvas id="doughnut" width="50" height="50" class="chart chart-doughnut"-->
                <!--chart-data="docs.cells_count" chart-labels="docs.cells">-->
                <!--</canvas>-->
            </td>
        </tr>

    </table>

    <div ng-if="datasets.totalDocuments > 10">
        <div ng-include="'/SmallMolecules/components/pagenation/pagination.html'"></div>
    </div>


</div>

<div class="tab-pane active row" ng-if="viewData.datasetinfo !=undefined">
    <div class="col-md-10 col-md-offset-1">
        <div class="row">
            <div class="row">
                <div class="col-md-6">
                    <div>
                        <div class="col-sm-2">
                            <b>LINCS ID</b>
                        </div>
                        <div class="space-bet">
                            {{viewData.datasetinfo.lincsidentifier.toString()}}
                        </div>
                        <div>
                            <div class="separator"></div>
                        </div>
                    </div>
                    <div>
                        <div class="col-sm-2">
                            <b>Name</b>
                        </div>
                        <div class="space-bet">
                            {{viewData.datasetinfo.SM_Name.toString()}}
                        </div>
                        <div>
                            <div class="separator"></div>
                        </div>
                    </div>
                    <div >
                        <div class="col-sm-2">
                            <b>Alternative Name</b>


                        </div>
                        <div class="space-bet">
                            {{viewData.datasetinfo.SM_Alternative_Name.toString()}}
                        </div>
                        <div>
                            <div class="separator"></div>
                        </div>
                    </div>
                    <div >
                        <div class="col-sm-2">
                            <b>Category</b>


                        </div>
                        <div class="space-bet">
                            {{viewData.datasetinfo.SM_Category.toString()}}
                        </div>
                        <div>
                            <div class="separator"></div>
                        </div>
                    </div>
                    <div>
                        <div class="col-sm-2">
                            <b>Center</b>
                        </div>
                        <div class="space-bet">
                            <div ng-repeat="aname in viewData.datasetinfo.source">
                                <a>{{aname | removeunderscore }}</a>

                            </div>
                        </div>
                        <div>
                            <div class="separator"></div>
                        </div>
                    </div>
                    <div>
                        <div class="col-sm-2">
                            <b>PubChem CID</b>


                        </div>
                        <div class="space-bet">
                            <a ng-href="https://pubchem.ncbi.nlm.nih.gov/compound/{{viewData.datasetinfo.SM_PubChem_CID}}" target="_blank">{{viewData.datasetinfo.SM_PubChem_CID}}</a>
                            <span class="titip-top" data-title="Opens link in a new tab"><i class="fa fa-external-link "   aria-hidden="true"></i></span>

                        </div>
                        <div>
                            <div class="separator"></div>
                        </div>
                    </div>
                    <div>
                        <div class="col-sm-2">
                            <b>Molecular Mass</b>


                        </div>
                        <div class="space-bet">
                            <div>
                                {{viewData.datasetinfo.SM_Molecular_Mass}}

                            </div>
                        </div>
                        <div>
                            <div class="separator"></div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <img ng-src="http://life.ccs.miami.edu/life/web/images/sm-images/400/{{viewData.datasetinfo.lincsidentifier.toString()}}.png">
                        <div style="margin-left: 5em"><button  style="background-color: #3a5278;border-color: #578fc7;color: whitesmoke" disabled >Substructure Search</button>
                            <button   style="background-color: #3a5278;border-color: #578fc7;color: whitesmoke" disabled>Similarity Search</button></div>
                    </div>
                </div>
            </div>
            <div>
                <div class="col-sm-2">
                    <b>SMILES Parent</b>


                </div>
                <div class="col-md-10 pull-left">
                    <div>
                        {{viewData.datasetinfo.SM_SMILES_Parent}}

                    </div>
                </div>
                <div>
                    <div class="separator"></div>
                </div>
            </div>
            <div>
                <div class="col-sm-2">
                    <b>InChi Parent</b>


                </div>
                <div class="col-md-10 pull-left">
                    <div>
                        {{viewData.datasetinfo.SM_InChi_Parent}}

                    </div>
                </div>
                <div>
                    <div class="separator"></div>
                </div>
            </div>
            <div>
                <div class="col-sm-2">
                    <b>Batch info</b>


                </div>
                <div class="col-md-10 pull-left">
                    <table id="table" style="margin-top: 1em" class="table table-bordered table-hover table-condensed table-responsive container-fluid">
                        <tr >
                            <th  class="smallmolecule" style="width: 15%">Center Sample ID</th>
                            <th   class="smallmolecule" style="width: 15%">Center Compound ID</th>
                            <th   class="smallmolecule" style="width: 20%">Provider</th>
                            <th   class="smallmolecule" style="width: 15%">Provider Catalog ID</th>
                            <th   class="smallmolecule" style="width: 40%">SMILES Batch</th>
                        </tr>
                        <tbody>
                        <tr ng-repeat="docs in viewData.samples">
                            <td ng-if="docs.SM_Center_Sample_ID != undefined"> {{docs.SM_Center_Sample_ID.toString()}}
                            </td>
                            <td ng-if="docs.SM_Center_Sample_ID != undefined">{{docs.SM_Center_Compound_ID.toString()}}
                            </td>
                            <td ng-if="docs.SM_Center_Sample_ID != undefined">
                                {{docs.SM_Provider.toString()}}
                            </td>
                            <td ng-if="docs.SM_Center_Sample_ID != undefined">
                                {{docs.SM_Provider_Catalog_ID.toString() }}

                            </td>
                            <td ng-if="docs.SM_Center_Sample_ID != undefined" >
                                {{docs.SM_SMILES_Batch.toString() }}
                            </td>

                        </tr>
                        </tbody>
                    </table>
                </div>
                <div>
                    <div class="separator"></div>
                </div>
            </div>
            <div>
                <div class="col-sm-2 ">
                    <b>Assays</b>


                </div>
                <div class="col-md-10 pull-left" style="margin-bottom: 0.3em">
                    <span ng-repeat="assay in viewData.datasetinfo.assays">
                        <span class="label label-default">{{assay }}</span>
                    </span>
                </div>
                <div>
                    <div class="separator"></div>
                </div>
            </div>
            <div >
                <div class="col-sm-2 ">
                    <b>Keywords</b>
                </div>
                <div class="col-md-10 pull-left">
                     <span ng-repeat="cat in viewData.datasetinfo.SM_Category">
                         <a ng-href="/SmallMolecules/#?query={{cat }}" target="_blank" ><span
                                 class="label label-danger">{{cat }}</span></a>
                     </span>
                     <span ng-repeat="assay in viewData.datasetinfo.assays">
                        <a ng-href="/SmallMolecules/#?query={{assay }}" target="_blank" ><span
                                class="label label-danger">{{assay }}</span></a>

                     </span>
                    <span class="titip-top" data-title="Opens link in a new tab"><i class="fa fa-external-link "
                                                                                    aria-hidden="true"></i></span>

                </div>
                <div>
                    <div class="separator"></div>
                </div>
            </div>


        </div>
        <br>
    </div>
</div>
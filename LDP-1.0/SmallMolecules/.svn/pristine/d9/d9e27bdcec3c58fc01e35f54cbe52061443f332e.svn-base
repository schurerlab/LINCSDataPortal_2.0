.nav-tabs {
  border-bottom: none;
}
a.disabled {
  pointer-events: none;
  cursor: default;
  color: grey;
}

.nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover {
  color: #555;
  cursor: default;
  background-color: white;
  border: 1px solid white;
  border-bottom-color: transparent;
}
